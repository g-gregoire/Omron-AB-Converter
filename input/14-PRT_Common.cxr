      PRT3_DRAIN_IND	BOOL	H301.09	PRT3 Drain Indicator		0	
      PRT4_DRAIN_IND	BOOL	H501.09	PRT4 Drain Indicator		0	
      PRT1_DRAIN_IND	BOOL	H101.09	PRT1 Drain Indicator		0	
      PRT3_DRAIN_REQ	BOOL	H301.08	PRT3 Drain Request		0	
      PRT1_STEP	WORD	D32100	PRT1 STEP NUMBER		0	
      PRT2_DRAIN_REQ	BOOL	H201.08	PRT2 Drain Request		0	
      PRT4_DRAIN_REQ	BOOL	H501.08	PRT4 Drain Request		0	
      PRT1_ACID_REQ	BOOL	H101.10	PRT1 Acid Indicator		0	
      PRT2_ACID_IND	BOOL	H201.11	PRT2 Acid Indicator		0	
      PRT3_ACID_IND	BOOL	H301.11	PRT3 Acid Indicator		0	
      PRT4_ACID_IND	BOOL	H501.11	PRT4 Acid Indicator		0	
      PRT1_ACID_IND	BOOL	H101.11	PRT1 Acid Indicator		0	
      PRT2_ACID_REQ	BOOL	H201.10	PRT2 Acid Indicator		0	
      PRT3_ACID_REQ	BOOL	H301.10	PRT3 Acid Indicator		0	
      PRT4_ACID_REQ	BOOL	H501.10	PRT4 Acid Indicator		0	
      PRT1_CAUSTIC_REQ	BOOL	H108.08	PRT1 Caustic Request		0	
      PRT2_CAUSTIC_IND	BOOL	H208.09	PRT2 Caustic Indicator		0	
      PRT3_CAUSTIC_IND	BOOL	H308.09	PRT3 Caustic Indicator		0	
      PRT4_CAUSTIC_IND	BOOL	H508.09	PRT4 Caustic Indicator		0	
      CAUSTIC_IND	BOOL	H357.15	Caustic General Indicator		0	
      PRT1_CAUSTIC_IND	BOOL	H108.09	PRT1 Caustic Indicator		0	
      PRT2_CAUSTIC_REQ	BOOL	H208.08	PRT2 Caustic Request		0	
      PRT3_CAUSTIC_REQ	BOOL	H308.08	PRT3 Caustic Request		0	
      PRT4_CAUSTIC_REQ	BOOL	H508.08	PRT4 Caustic Request		0	
      LIT_1002_AI	CHANNEL	D32104	PRT1 Level Transmitter		0	
      	BOOL	W18.12	PRT#1 > 10%		0	
      LIT_2002_AI	CHANNEL	D32204	PRT2 Level Transmitter		0	
      	BOOL	W18.13	PRT#2 > 10%		0	
      LIT_3002_AI	CHANNEL	D32304	PRT3 Level Transmitter		0	
      	BOOL	W18.14	PRT#3 > 10%		0	
      LIT_4002_AI	CHANNEL	D32504	PRT4 Level Transmitter		0	
      	BOOL	W18.15	PRT#4 > 10%		0	
      ON	BOOL	CF113	System Normally On Flag		0	
      	CHANNEL	D273	Day		0	
      	BOOL	W17.00	One or more PRT's Draining		0	
      _1_SEC_PULSE	BOOL	CF102	System 1.0 second clock pulse bit		0	
